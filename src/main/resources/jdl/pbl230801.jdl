    lstMtnUsr String,
    lstMtnDt ZonedDateTime,
}

entity MatchPlayer {
    mId Long,
    pId Long,
    eId Long,
    mtchEndTime ZonedDateTime,
    score String,
    winFg String,
    lstMtnUsr String,
    lstMtnDt ZonedDateTime,
}

@readOnly
entity VwEventResult {
    eId Long,
    pId Long,
    mId Long,
    winFg String,
    plyrNm String,
    plyrLvl String,
    mtchEndTime ZonedDateTime,
    totMatchs String,
