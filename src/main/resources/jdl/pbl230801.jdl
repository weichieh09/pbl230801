    eId Long,
    pId Long,
    chkFg String,
    lstMtnUsr String,
    lstMtnDt ZonedDateTime,
}

entity MatchZ {
    eId Long,
    mtchEndTime ZonedDateTime,
    wPlyr1 String,
    wPlyr2 String,
    wScr String,
    lPlyr1 String,
    lPlyr2 String,
