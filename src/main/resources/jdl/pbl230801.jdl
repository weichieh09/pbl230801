entity Team {
    teamNm String,
    lstMtnUsr String,
    lstMtnDt ZonedDateTime,
}

entity Player {
    plyrNm String,
    plyrLvl String,
    lstMtnUsr String,
    lstMtnDt ZonedDateTime,
}

entity TeamPlayer {
    tId Long,
    pId Long,
    lstMtnUsr String,
    lstMtnDt ZonedDateTime,
}

entity EventZ {
    evntNm String,
    evntDt ZonedDateTime,
    venue String,
